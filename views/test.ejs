<h2>Products Table</h2>
<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Zip Code</th>
      <th>Edit</th>
      <th>Delete</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>
        <a href="<%= `/products/${product._id}` %>">
          <%= product.name %>
        </a>
      </td>
      <td>
        <%= product.picture %>
      </td>
      <td>
        <%= product.price %>
      </td>
    </tr>
  </tbody>
</table>
